    Layout = "~/Views/Shared/_AdminPage.cshtml";
}

@section css {
    <!-- JQVMap -->
<link href="~/Layout/Admin/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
<link href="~/Layout/Admin/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
<link href="~/Layout/Admin/vendors/nprogress/nprogress.css" rel="stylesheet">
<link href="~/Layout/Admin/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
